<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pickuplines"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.walter.pickuplines.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity android:name="com.walter.pickuplines.Search_Results" 
             android:parentActivityName=".MainActivity"/>
        <activity android:name="com.walter.pickuplines.Favour"
            android:parentActivityName=".MainActivity"/>
        <activity android:name="com.walter.pickuplines.Help_About"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>